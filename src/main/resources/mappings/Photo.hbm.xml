<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="model.entities">

    <class name="Photo" table="Photos">
        <id name="id" column="photo_id">
            <generator class="native"/>
        </id>

        <property name="photoName" length="2000"/>
        <property name="photoLocationBIG" length="2000"/>
        <property name="photoLocationMEDIUM" length="2000"/>
        <property name="photoLocationSMALL" length="2000"/>
        <property name="author"/>
        <property name="publishDate"/>
    </class>
</hibernate-mapping>

